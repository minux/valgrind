#! /bin/sh

dir=`dirname $0`

$dir/../../tests/filter_stderr_basic                |

# Remove numbers from all lines (and "(n/a)" strings)
sed "s/\(Total spacetime:   \).*$/\1/" |
sed "s/\(heap:              \).*$/\1/" |
sed "s/\(heap admin:        \).*$/\1/" |
sed "s/\(stack(s):          \).*$/\1/"

